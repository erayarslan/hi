{
  "name": "hi",
  "description": "Hepsiburada.com Insight",
  "short_name": "hi",
  "version": "0.0.1",
  "permissions": [
    "tabs",
    "notifications",
    "<all_urls>"
  ],
  "manifest_version": 2,
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "browser_action": {
    "default_icon": "assets/icon128.png"
  },
  "background": {
    "scripts": [
      "src/constants.js",
      "src/service.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "data/map.js",
        "data/data.js",
        "libs/isarray.js",
        "libs/path-to-regexp.js",
        "libs/multiline.js",
        "src/constants.js",
        "src/extend.js",
        "src/utils.js",
        "src/search.js",
        "src/app.js",
        "src/boot.js"
      ]
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/constants.js",
        "src/iframe.js"
      ],
      "all_frames": true
    }
  ]
}